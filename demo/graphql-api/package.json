{
    "name": "nestjs-apollo-graphql",
    "version": "1.0.0",
    "description": "This is a nestjs graphql demo.",
    "main": "main.ts",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:watch": "nodemon",
        "debug": "node --inspect-brk -r ts-node/register src/main.ts",
        "debug:watch": "nodemon --config nodemon-debug.json",
        "check": "tslint -p tsconfig.json -c tslint.json",
        "fix": "tslint -p tsconfig.json -c tslint.json --fix"
    },
    "author": "dzzzzzy",
    "license": "MIT",
    "dependencies": {
        "@nestjs/common": "^5.7.2",
        "@nestjs/core": "^5.7.2",
        "@nestjs/graphql": "^5.5.2",
        "@nestjs/typeorm": "^6.0.0",
        "apollo-server-express": "^2.4.2",
        "graphql": "^14.1.1",
        "graphql-subscriptions": "^1.0.0",
        "graphql-tools": "^4.0.4",
        "pg": "^7.8.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.4.0",
        "subscriptions-transport-ws": "^0.9.15",
        "typeorm": "^0.2.13",
        "typescript": "^3.3.3"
    },
    "devDependencies": {
        "@types/graphql": "^14.0.7",
        "@types/node": "^11.9.4",
        "nodemon": "^1.18.10",
        "ts-node": "^8.0.2",
        "tsconfig-paths": "^3.8.0",
        "tslint": "^5.12.1"
    }
}