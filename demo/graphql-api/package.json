        "@nestjs/graphql": "^5.4.0",
        "@nestjs/typeorm": "^5.2.2",
        "apollo-server-express": "^2.2.6",
        "graphql": "^14.0.2",
        "graphql-subscriptions": "^1.0.0",
        "graphql-tools": "^4.0.3",
        "pg": "^7.7.1",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.3",
        "subscriptions-transport-ws": "^0.9.15",
        "typeorm": "^0.2.9",
        "typescript": "^3.2.2"
    },
    "devDependencies": {
        "@types/graphql": "^14.0.3",
