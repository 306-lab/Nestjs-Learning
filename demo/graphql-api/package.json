        "apollo-server-express": "^2.0.4",
        "graphql": "^14.0.0",
        "graphql-subscriptions": "^0.5.8",
        "graphql-tools": "^3.1.1",
        "pg": "^7.4.3",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.0",
        "subscriptions-transport-ws": "^0.9.14",
        "typeorm": "^0.2.7",
        "typescript": "^3.0.3"
    },
    "devDependencies": {
        "@types/graphql": "^0.13.4",
