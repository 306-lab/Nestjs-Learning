{
    "name": "nestjs-apollo-graphql-v2",
    "version": "1.0.0",
    "description": "This is a nestjs graphql demo.",
    "main": "main.ts",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
